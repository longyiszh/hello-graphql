{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictNullChecks": true
  },

  "exclude": [
    "node_modules",
    "dist"
  ]
}